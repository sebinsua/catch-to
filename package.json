{
  "name": "catch-to",
  "version": "0.2.0",
  "description": "Catch errors and declaratively map them to other errors or values.",
  "main": "index.js",
  "repository": "git@github.com:sebinsua/catch-to.git",
  "author": "Seb Insua <me@sebinsua.com>",
  "license": "MIT",
  "keywords": [
    "catch",
    "promise",
    "api",
    "errors",
    "map",
    "boom",
    "value"
  ],
  "scripts": {
